{
  "name": "bookmark-search",
  "description": "Get the bookmark you want.",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": [],
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "persistent": true
}