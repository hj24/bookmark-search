{
  "name": "bookmark-search",
  "description": "Find the bookmark you want",
  "version": "1.2.1",
  "manifest_version": 3,
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "persistent": true,
  "commands": {
    "_execute_action": {
        "suggested_key": {
            "mac": "Command+Shift+S",
            "windows": "Ctrl+Shift+S",
            "chromeos": "Ctrl+Shift+S",
            "linux": "Ctrl+Shift+S"
        }
    }
  }
}
